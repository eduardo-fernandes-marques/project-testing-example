  "scripts": {
    "dev": "NODE_ENV='development' node configs/server.js",
    "next:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "DEBUG_PRINT_LIMIT=300000 jest --config configs/test/jest.config.js --watchAll=false --maxWorkers=2",
    "test:cov": "yarn test --coverage --watchAll=false",
    "test:ci": "yarn test --maxWorkers=2 --passWithNoTests --ci --no-cache --reporters=default --reporters=jest-junit --silent",
    "eslint": "eslint --config configs/.eslintrc.json src --ext .js,.ts,.tsx",
    "typecheck": "tsc -p . --noEmit",
    "pre-commit": "yarn prettier && yarn typecheck & yarn eslint --quiet & yarn test",
    "prettier": "prettier --config configs/.prettierrc.json --write **/*.{ts,tsx}"
  },
